{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "ignorePatterns": ["*.css", "**/vendor/**"],
    "rules": {
        "no-console": "off",
        "react/prop-types": "off",
        "react/jsx-props-no-spreading": "off",
        "arrow-body-style":  "off",
        "jsx-a11y/label-has-associated-control": "off",
        "import/no-extraneous-dependencies": "off"
    }
}
